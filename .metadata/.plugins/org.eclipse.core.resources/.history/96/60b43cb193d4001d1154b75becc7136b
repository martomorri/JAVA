package cursojava;
import javax.swing.*;

public class Factorial {

	public static void main(String[] args) {
		int num = Integer.parseInt(JOptionPane.showInputDialog("Ingrese un numero: "));
		int factorial = 0;
		for (int i = num-1; i > 0; i--) {
			factorial = num * i;
		}
		System.out.println(factorial);
	}

}
